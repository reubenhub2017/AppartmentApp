<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reubenukah.appartmentapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".options">


            <intent-filter>
                <action android:name="android.intent.action.MAIN" />


                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".Displaylandlord"
            android:label="Landlord Sign Up "
            android:parentActivityName=".options"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Displaytenet"
            android:label="Tenet Sign Up "
            android:parentActivityName=".options"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ImageUpload" />
        <activity
            android:name=".HomeScreen"
            android:label="@string/title_activity_home_screen"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>